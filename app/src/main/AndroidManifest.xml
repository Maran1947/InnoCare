<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.innocare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InnoCare">
        <activity android:name=".TakeAFoodActivity"></activity>
        <activity android:name=".AddFoodActivity" />
        <activity android:name=".TrackCalorieActivity" />
        <activity android:name=".CategoriesActivity" />
        <activity android:name=".WeeklyReportActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".SigninActivity" />
        <activity android:name=".SignupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>